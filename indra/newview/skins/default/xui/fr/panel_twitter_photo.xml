<?xml version="1.0"?>
<panel name="panel_twitter_photo">
	<text name="status_label">Quoi de neuf ?</text>
	<check_box label="Inclure l'emplacement sur SL" name="add_location_cb"/>
	<check_box label="Joindre une photo" name="add_photo_cb"/>
	<combo_box name="resolution_combobox" tool_tip="Résolution de l'image">
		<combo_box.item label="Fenêtre actuelle" name="CurrentWindow"/>
		<combo_box.item label="Personnalisée" name="Custom"/>
	</combo_box>
	<combo_box name="filters_combobox" tool_tip="Filtre de l'image">
		<combo_box.item label="Pas de filtre" name="NoFilter"/>
	</combo_box>
	<check_box label="Ratio conservé" name="keep_aspect_ratio"/>
	<text name="working_lbl">Actualisation...</text>
	<button label="Actualiser" name="new_snapshot_btn" tool_tip="Cliquez pour actualiser"/>
	<button label="Prévisualiser" name="big_preview_btn" tool_tip="Cliquez pour voir un aperçu"/>
	<button label="Tweeter" name="post_photo_btn"/>
	<button label="Annuler" name="cancel_photo_btn"/>
</panel>
